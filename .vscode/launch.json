{
    "version": "0.2.0",
    "configurations": [
        
    {
        "type": "node",
        "request": "launch",
        "name": "Gulp task",
        "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
        "args": [
            "task"
        ]
    },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:9005",
            "webRoot": "${workspaceRoot}"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }
    ]
}